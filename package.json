{
  "name": "docker-transparent-http-cache",
  "version": "0.0.2",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "commander": "2.0.0",
    "log4js": "0.6.8",
    "memorystream": "^0.3.1",
    "pem": "^1.7.2",
    "request": "2.27.0",
    "restify": "^4.0.0"
  },
  "description": "HTTP/HTTPS caching proxy for Docker build containers",
  "main": "index.js",
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "jquery-ajax": "^2.1.4",
    "mocha": "*",
    "react": "~0.13.3",
    "reactable": "^0.10.2",
    "rimraf": "*",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3"
  },
  "scripts": {
    "start": "node bin/docker-transparent-http-cache",
    "test": "mocha -R spec --recursive",
