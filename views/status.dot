<html>
    <h1>Cache status</h1>

    <ul>
        <li>Cache entries: {{=it.cache.size}}</li>
        <li>Cache total size: {{= (function() { var s = 0; for(var value of it.cache.values()) { s = s + value.data.length; }; return s; })() }}</li>
    </ul>

    <form action="/" method="post">
        <button type="submit">Invalidate</button>
    </form>

    <hr/>

    <table border="1">
        {{ for(var url of it.cache.keys()) { }}
        {{ var value = it.cache.get(url); }}
            <tr><td>{{=url}}</td><td>{{=value.etag}}</td><td>{{=value.contenttype}}</td><td>{{=value.data.length}}</td></tr>
        {{ } }}
    </table>
</html>